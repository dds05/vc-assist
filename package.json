{
  "name": "vc-assist",
  "private": true,
  "version": "1.0.0",
  "description": "Full stack app with Next.js frontend and Go backend",
  "scripts": {
    "frontend:dev": "cd frontend && npm run dev",
    "frontend:build": "cd frontend && npm run build",
    "frontend:start": "cd frontend && npm run start",
    "backend:dev": "cd backend && go run main.go",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write .",
    "clean": "rimraf frontend/node_modules frontend/.next"
  },
  "devDependencies": {
    "eslint": "^9.10.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "license": "MIT"
}
